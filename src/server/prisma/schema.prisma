model SurveyResult {
  id             String   @id @default(cuid()) @db.VarChar(30)
  surveyId       String   @db.VarChar(30)
  createdAt      DateTime @default(now()) @db.Timestamptz(6)
  sessionId      String   @db.Uuid
  /// [CommonPayload]
  /// @zod.custom(imports.CommonPayloadSchema)
  payload        Json     @db.Json
